<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="buckets.framework.dictionary.core.dao.DictionaryDao">

    <select id="countByCode" parameterType="java.lang.String" resultType="java.lang.Integer">
        select count(1) from dictionary where code = #{code};
    </select>

    <select id="findByCode" parameterType="java.lang.String" resultType="buckets.framework.dictionary.entity.Dictionary">
        select * from dictionary where code = #{code};
    </select>
</mapper>